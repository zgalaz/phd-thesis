%% Vysazeni seznamu zkratek a symbolu
\newenvironment{seznamzkratek}[1]%
  {%
    \cleardoublepage
    \chapter*{\listofabbrevname}
    \phantomsection
    \addcontentsline{toc}{chapter}{\listofabbrevname}
    \begin{acronym}[#1]
    % zruseni dodatecnych mezer mezi symboly
    \setlength\itemsep{-\parsep}
  }{%
    \end{acronym}
    \newpage

    \if@twoside
      \pagestyle{empty}
      \cleardoublepage
    \fi
  }%

